<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dubbo.shop.mapper.TGoodsBaseMapper">

    <select id="list" resultType="com.dubbo.shop.entity.TGoodsBase">

        select *
        from t_goods_base
        where 1 = 1

    </select>

    <select id="findByConditions" parameterType="com.dubbo.shop.entity.TGoodsBase" resultType="com.dubbo.shop.entity.TGoodsBase">

        select *
        from t_goods_base
        where 1 = 1
        and type_name = #{type_name}
        and name = ${name}

    </select>

</mapper>
